<IfModule mod_rewrite.c>
    <IfModule mod_negotiation.c>
        Options -MultiViews -Indexes
    </IfModule>

    RewriteEngine On

    # Redirect Request To Https 
    #RewriteCond %{SERVER_PORT} 80
   # RewriteRule ^(.*)$ https://%{HTTP_HOST}%{REQUEST_URI} [R,L]

    # Redirect Request To www 
   # RewriteCond %{HTTP_HOST} ^[^.]+\.[^.]+$
    #RewriteCond %{HTTPS}s ^on(s)|
    #RewriteRule ^ http%1://www.%{HTTP_HOST}%{REQUEST_URI} [L,R=301]

    # Handle Authorization Header
    RewriteCond %{HTTP:Authorization} .
    RewriteRule .* - [E=HTTP_AUTHORIZATION:%{HTTP:Authorization}]

    # Redirect Trailing Slashes If Not A Folder...
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_URI} (.+)/$
    RewriteRule ^ %1 [L,R=301]

    # Redirect if index.php is in the URL
    RewriteCond %{THE_REQUEST} /index\.php [NC]
    RewriteCond %{REQUEST_URI} ^(.*/)index\.php$ [NC]
    RewriteRule ^ %1 [L,R=301,NE]
    RewriteRule ^index.php(/?)(.+) $1$2 [R=301,L]

    # Send Requests To Front Controller...
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteRule ^ index.php [L]


    #Redirect 301 /info/inscriere-in-programul-casa-verde https://www.genway.ro/info/oferta-casa-verde-2023
    Redirect 301 /info/inscriere-in-program https://www.genway.ro/info/oferta-casa-verde-2023
    Redirect 301 /info/oferta-noastra-electricup https://www.genway.ro/info/inscrierea-in-programul-electricup

</IfModule>
<IfModule mod_php8.c>
    php_value memory_limit 64M
</IfModule>

